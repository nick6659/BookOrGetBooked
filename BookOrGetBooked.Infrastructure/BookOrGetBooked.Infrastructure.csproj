    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Data\SeedData\currencies.json" />
    <None Remove="Data\SeedData\services.json" />
    <None Remove="Data\SeedData\users.json" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Data\SeedData\currencies.json" />
